@model IEnumerable<AcroGARC.Models.ClassStructure>
@{
    ViewBag.Title = "GetCourseClasses";
}

<ol class="breadcrumb">
    <li class="breadcrumb-item">@Html.ActionLink("Course", "Index", "Course")</li>
    @*<li class="breadcrumb-item active">@Model.CourseName</li>*@
    
</ol>
<table class="table table-hover">
    <thead>
        <tr>
            <th>Semester</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
            {
            <tr>
                <td>
                   @Html.ActionLink(obj.Semester.Name,"Index","Class",new { id = obj.Id},null)

                </td>
                <td>
                    
                </td>               
            </tr>
        }
    </tbody>

</table>



